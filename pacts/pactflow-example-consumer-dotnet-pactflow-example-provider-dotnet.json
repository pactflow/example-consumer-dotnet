{
  "consumer": {
    "name": "pactflow-example-consumer-dotnet"
  },
  "interactions": [
    {
      "description": "A request to product by Id",
      "providerStates": [
        {
          "name": "product id 27 exist"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/product/2"
      },
      "response": {
        "body": [
          {
            "id": "27",
            "name": "burger",
            "type": "food"
          }
        ],
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    },
    {
      "description": "A request to get products",
      "providerStates": [
        {
          "name": "products exist"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/products"
      },
      "response": {
        "body": [
          {
            "id": "27",
            "name": "burger",
            "type": "food"
          }
        ],
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.4.0",
      "models": "1.0.4"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "pactflow-example-provider-dotnet"
  }
}